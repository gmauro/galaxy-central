<tool id="EMBOSS: econtrast4" name="EMBOSS: econtrast" version="3.0.0">
  <description>Computes contrasts for comparative method</description>
  <command>econtrast -infile $input1 -outfile $out_file1 -treefile $input2 -datasets "$datasets" -corplusreg $corplusreg -multsets $multsets -printdata $printdata -progress no -auto</command>
  <inputs>
    <param format="data" name="input1" type="data">
      <label>Input file</label>
    </param>
    <param format="data" name="input2" type="data">
      <label>Input tree file</label>
    </param>
    <param name="datasets" size="5" type="text" value="1">
      <label>How many data sets</label>
    </param>
    <param name="corplusreg" type="select">
      <label>Print out correlations and regressions</label>
      <option value="yes">Yes</option>
      <option value="no">No</option>
    </param>
    <param name="multsets" type="select">
      <label>Analyze multiple data sets</label>
      <option value="no">No</option>
      <option value="yes">Yes</option>
    </param>
    <param name="printdata" type="select">
      <label>Print out the data at start of run</label>
      <option value="no">No</option>
      <option value="yes">Yes</option>
    </param>
  </inputs>
  <outputs>
    <data format="txt" name="out_file1" />
  </outputs>
  <help>.. meta::
   :HTTP-EQUIV=Refresh: 0; URL=http://emboss.sourceforge.net/embassy/phylip/econtrast.html

You are now being redirected to the original documentation.

Please click http://emboss.sourceforge.net/embassy/phylip/econtrast.html if you are not automatically redirected.
</help>
</tool>